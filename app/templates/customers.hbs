<h2>Customer List</h2>

<table>
  <th>
    <tr>
      <td>ID</td>
      <td>Name</td>
      <td>Phone</td>
    </tr>
  </th>
  <tbody>
    {{#each customer in model}}
    <tr>
      <td>{{customer.id}}</td>
      <td>
        {{#link-to 'customer' customer}}
        {{customer.name}}
        {{/link-to}}
      </td>
      <td>{{customer.phone}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

{{outlet}}
